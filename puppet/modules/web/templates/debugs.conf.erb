<Location />
  AuthType Basic
  AuthName "Foreman-debug uploads - restricted area"
  AuthUserFile /var/www/vhosts/debugs/htpasswd
  Require valid-user
</Location>

# For ACME/Let's Encrypt challenges
<Location /.well-known>
  Satisfy Any
  Order Allow,Deny
  Allow from all
</Location>

# For AWStats icons
Alias "/awstats/icon/" "/usr/share/awstats/wwwroot/icon/"
